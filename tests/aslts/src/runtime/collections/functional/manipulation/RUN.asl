            DCM0 ()
            SRMT ("TOI0")    /* Convert data to integer */
            TOI0 ()
            SRMT ("TOS0")    /* Convert buffer to string */
            TOS0 ()
            SRMT ("TOB0")    /* Convert Data to Buffer */
            TOB0 ()
